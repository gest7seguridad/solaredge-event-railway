{
  "name": "solaredge-event-registration",
  "version": "1.0.0",
  "description": "Sistema de inscripciÃ³n para evento presencial SolarEdge - Solarland",
  "main": "backend/dist/server.js",
  "scripts": {
    "start": "node backend/dist/server.js",
    "start:production": "node backend/dist/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "node build.mjs",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:deps": "cd backend && npm install && cd ../frontend && npm install && cd ..",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "author": "Solarland",
  "license": "MIT"
}
